.body {
  width: 750rpx;
  height: 1395rpx;
  background: linear-gradient(90deg, #128F8F 0%, #1FB0B0 47%, #13ACAC 63%, #169393 100%);
}
text {
  position: absolute;
  left: 64rpx;
  font-size: 50rpx;
  top: 130rpx;
  font-family: Jost-Black,jost;
  font-size: 50rpx;
  font-weight: 800;
  color: #FFFFFF;
  line-height: 75rpx;
}
.log {
  width: 750rpx;
  height: 1000rpx;
  position: absolute;
  bottom: 0;
}
.form {
  width: 737rpx;
  height: 990rpx;
  background-color: white;
  border-top-left-radius: 60rpx;
  border-top-right-radius: 60rpx;
  display: flex;
  flex-direction: column;
  border: 8rpx solid #D9D9D9;
  border-bottom: none;
}
.input-text {
  width: 626rpx;
  height: 100rpx;
  margin: 60rpx auto;
  background-color: #FFFFFF;
}
.input-text .title {
  font-size: 35rpx;
  font-weight: 600;
}
.input-text input {
  margin-top: 50rpx;
  border-bottom: 6rpx solid #F5F5F5;
  height: 80rpx;
}
.input-text input .plc {
  font-size: 36rpx;
  font-family: Jost-Regular, Jost;
  font-weight: 400;
  color: #D9D9D9;
}
.eyes {
  width: 50rpx;
  height: 50rpx;
  position: relative;
  bottom: 60rpx;
  left: 570rpx;
}
.eyes image {
  width: 100%;
  height: 100%;
}
.login {
  position: relative;
  top: 300rpx;
  width: 604rpx;
  height: 122rpx;
  background: linear-gradient(180deg, #969290 0%, #D1D1D1 34%, #D1D1D1 71%, #B2AFAB 100%);
  border-radius: 38rpx;
  opacity: 1;
  border: 6rpx solid #F3EFEF;
  padding-top: 15rpx;
  font-size: 62rpx;
  font-family: Jost-Light, Jost;
  font-weight: 600;
  color: #000000;
  line-height: 72rpx;
}
.signUp {
  margin: 0 auto;
  bottom: 50rpx;
}
.signUp ._span {
  color: blue;
  border-bottom: 5rpx solid blue;
}

